/**app.wxss**/
/* 全局引入的css 可以全局访问
  样式框架 weui-cell
  colorui 色彩和样式
  -apple-system: 苹果字体
*/
@import "weui/weui.wxss";
@import "colorui/main.wxss";
@import "colorui/icon.wxss";
@import "colorui/animation.wxss";

page {
  font-size: 16px;
  font-family: -apple-system, 'Helvetica Neue', sans-serif;
  background-color: #f8f8f8;
}
.page {
  width: 100%;
  height: 100vh;
  overflow-x: hidden;
}
